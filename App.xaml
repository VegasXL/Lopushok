<Application x:Class="Osipov.Lopushok.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Osipov.Lopushok"
             StartupUri="Presentation/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="ColorPrimary">#FFF</SolidColorBrush>
            <SolidColorBrush x:Key="ColorSecondary">#CEFFf9</SolidColorBrush>
            <SolidColorBrush x:Key="ColorAccent">#00CC76</SolidColorBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>
